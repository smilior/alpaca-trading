<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.alpaca-trading.eod</string>
    <key>ProgramArguments</key>
    <array>
        <string>__VENV_PATH__/bin/python</string>
        <string>__PROJECT_PATH__/main.py</string>
        <string>eod</string>
    </array>
    <key>WorkingDirectory</key>
    <string>__PROJECT_PATH__</string>
    <!-- 20:05/21:05 UTC = 16:05 ET (EDT/EST) — 市場終了5分後 -->
    <key>StartCalendarInterval</key>
    <array>
        <dict>
            <key>Hour</key>
            <integer>20</integer>
            <key>Minute</key>
            <integer>5</integer>
        </dict>
        <dict>
            <key>Hour</key>
            <integer>21</integer>
            <key>Minute</key>
            <integer>5</integer>
        </dict>
    </array>
    <key>StandardOutPath</key>
    <string>__PROJECT_PATH__/logs/launchd-eod.log</string>
    <key>StandardErrorPath</key>
    <string>__PROJECT_PATH__/logs/launchd-eod-error.log</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>__VENV_PATH__/bin:__HOME_PATH__/.local/bin:/usr/local/bin:/usr/bin:/bin</string>
    </dict>
</dict>
</plist>
